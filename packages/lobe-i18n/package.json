{
  "name": "@lobehub/i18n-cli",
  "version": "1.7.2",
  "description": "Lobe i18n is a CLI tool that automate translate your i18n localization with AI",
  "keywords": [
    "ai",
    "i18n",
    "openai",
    "gpt",
    "langchain"
  ],
  "homepage": "https://github.com/lobehub/lobe-commit/tree/master/packages/lobe-i18n",
  "bugs": {
    "url": "https://github.com/lobehub/commit-cli/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lobehub/commit-cli.git"
  },
  "license": "MIT",
  "author": "canisminor1990 <i@canisminor.cc>",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "@": "./src"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "lobe-i18n": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify -p tsconfig.prod.json --env.NODE_ENV=production && npm run shebang",
    "dev": "pkgroll -p tsconfig.prod.json --env.NODE_ENV=development --watch",
    "link": "npm run build && npm link -f",
    "prepack": "npm run build && clean-pkg-json",
    "prepublishOnly": "npm run build",
    "shebang": "node ../common/add-shebang.js ./dist/cli.js",
    "start": "node ./dist/cli.js",
    "test": "npm run type-check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@inkjs/ui": "^1",
    "@lobehub/cli-ui": "1.8.1",
    "chalk": "^5",
    "commander": "^11",
    "conf": "^11",
    "consola": "^3",
    "cosmiconfig": "^8",
    "fast-deep-equal": "^3",
    "gpt-3-encoder": "^1",
    "ink": "^4.4",
    "langchain": "latest",
    "lodash-es": "^4",
    "pangu": "^4",
    "react": "^18",
    "update-notifier": "^6",
    "zustand": "^4"
  },
  "peerDependencies": {
    "ink": ">=4",
    "react": ">=18"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
